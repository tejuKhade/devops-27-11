pipeline {
    agent any
    stages {
    stage('pull-code') {
        steps {
            sh 'printenv'
        }
    }
    stage ('printenv') {
        steps {
            echo "build no is ${env.BUILD_ID}"
            echo "branch on which git present ${env.GIT_BRANCH}"
            echo "build no is ${env.NODE_NAME}"
}
}
    }
}